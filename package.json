{
  "name": "library-audit",
  "version": "0.1.7",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "library-audit",
    "manifest": "src/manifest.json",
    "main": "library-audit.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naughtandcross/sketch-library-audit"
  },
  "author": "Colby Grenier <colby.grenier@gmail.com>"
}
